{
    "log": {
        "loglevel": "warning"
    },
    "inbounds": [
        {
            "port": 443,
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "b667c36d-fd75-4854-9fe1-2b1ef802ceab", // 填写你的 UUID
                        "flow": "xtls-rprx-direct",
                        "level": 0,
                        "email": "love@a.com"
                    }
                ],
                "decryption": "none",
                "fallbacks": [
                    {
                        "dest": 1310, // 默认回落到 Xray 的 Trojan 协议
                        "xver": 1
                    },
                    {
                        "path": "/vlessws80", // 必须换成自定义的 PATH，可用cloudflare flexible mode，必须用80端口
                        "dest": 80,
                        "xver": 1
                    },
                    {
                        "path": "/vlessws", // 必须换成自定义的 PATH
                        "dest": 1234,
                        "xver": 1
                    },
                    {
                        "path": "/vmesstcp", // 必须换成自定义的 PATH
                        "dest": 2345,
                        "xver": 1
                    },
                    {
                        "path": "/vmessws", // 必须换成自定义的 PATH
                        "dest": 3456,
                        "xver": 1
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "xtls",
                "xtlsSettings": {
                    "alpn": [
                        "http/1.1"
                    ],
                    "certificates": [
                        {
                            "certificateFile": "/usr/local/etc/xray/cert.crt", // 换成你的证书，绝对路径
                            "keyFile": "/usr/local/etc/xray/cert.key" // 换成你的私钥，绝对路径
                        }
                    ]
                }
            }
        },
        {
            "port": 1310,
            "listen": "127.0.0.1",
            "protocol": "trojan",
            "settings": {
                "clients": [
                    {
                        "password": "", // 填写你的密码
                        "level": 0,
                        "email": "love@a.com"
                    }
                ],
                "fallbacks": [
                    {
                        "dest": "www.football4football.com:443" // 或者回落到其它也防探测的代理
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "none",
                "tcpSettings": {
                    "acceptProxyProtocol": true
                }
            }
        },
        {
            "port": 80,
            //"listen": "127.0.0.1",  //允许cloudflare访问80端口，所以必须删掉这行
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "43f7592d-3dc5-4022-88a0-8d75c9c166f6", // 填写你的 UUID
                        "level": 0,
                        "email": "love@a.com"
                    }
                ],
                "decryption": "none",
                "fallbacks": [
                    {
                        "dest": "www.descreen.net:443" // 或者回落到其它也防探测的代理
                    }
                ]
            },
            "streamSettings": {
                "network": "ws",
                "security": "none",
                "wsSettings": {
                    //"acceptProxyProtocol": true, // 提醒：若你用 Nginx/Caddy 等反代 WS，需要删掉这行。用lcoudflare反代WS，也要删掉这行
                    "path": "/vlessws80" // 必须换成自定义的 PATH，需要和分流的一致
                }
            }
        },
        {
            "port": 1234,
            "listen": "127.0.0.1",
            "protocol": "vless",
            "settings": {
                "clients": [
                    {
                        "id": "b667c36d-fd75-4854-9fe1-2b1ef802ceab", // 填写你的 UUID
                        "level": 0,
                        "email": "love@a.com"
                    }
                ],
                "decryption": "none"
            },
            "streamSettings": {
                "network": "ws",
                "security": "none",
                "wsSettings": {
                    "acceptProxyProtocol": true, // 提醒：若你用 Nginx/Caddy 等反代 WS，需要删掉这行
                    "path": "/vlessws" // 必须换成自定义的 PATH，需要和分流的一致
                }
            }
        },
        {
            "port": 2345,
            "listen": "127.0.0.1",
            "protocol": "vmess",
            "settings": {
                "clients": [
                    {
                        "id": "b667c36d-fd75-4854-9fe1-2b1ef802ceab", // 填写你的 UUID
                        "level": 0,
                        "email": "love@a.com"
                    }
                ]
            },
            "streamSettings": {
                "network": "tcp",
                "security": "none",
                "tcpSettings": {
                    "acceptProxyProtocol": true,
                    "header": {
                        "type": "http",
                        "request": {
                            "path": [
                                "/vmesstcp" // 必须换成自定义的 PATH，需要和分流的一致
